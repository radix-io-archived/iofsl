AM_CPPFLAGS = -I$(top_srcdir)/src \
	      -I$(top_srcdir)/src/iofwdutil \
	      -I$(top_srcdir)/src/zoidfs \
	      @BOOST_CPPFLAGS@ \
	      @FUSE_CPPFLAGS@ \
	      @ZFS_DISPATCHER_CPPFLAGS@ \
	      @BMI_CPPFLAGS@ \
          @CUNIT_CPPFLAGS@ \
	  @ATOMICS_CPPFLAGS@
	
AM_CFLAGS = @FUSE_CFLAGS@ @ZFS_DISPATCHER_CFLAGS@ @ATOMICS_CFLAGS@

AM_LIBS = @ATOMICS_LIBS@

AM_CXXFLAGS = $(AM_CFLAGS)

